!      SUBROUTINE W3SIS2 (A, ICE, DMAX, TAUISX, TAUISY, S)
      SUBROUTINE W3SIS2 (A, CICE, ICEH, ICEDAVE, S, D)
!/
!/                  +-----------------------------------+
!/                  | WAVEWATCH III           NOAA/NCEP |
!/                  |   P. Nicot & F. Ardhuin           |
!/                  |                        FORTRAN 90 |
!/                  | Last update :         16-Mar-2014 |
!/                  +-----------------------------------+
!/
!/    16-Mar-2014 : Origination.                        ( version 4.18 )
!
!  1. Purpose :
!     Wave scattering in the MIZ, adapted from Dumont et al.  
!
!/ ------------------------------------------------------------------- /
!
!  2. Method :
!
!  3. Parameters :
!
!     Parameter list
!     ----------------------------------------------------------------
!       A       R.A.  I   Action density spectrum (1-D)
!       CICE    Real  I   Sea ice concentration.
!       ICEDAVE Real  I   Average floe size (see Williams et al. 2013) 
!       S       R.A.  O   Source term (1-D version).
!     ----------------------------------------------------------------
!
!  4. Subroutines used :
!
!      Name      Type  Module   Description
!     ----------------------------------------------------------------
!     ----------------------------------------------------------------
!
!  5. Called by :
!
!      Name      Type  Module   Description
!     ----------------------------------------------------------------
!      W3SRCE    Subr. W3SRCEMD Source term integration.
!      W3EXPO    Subr.   N/A    ASCII Point output post-processor.
!      W3EXNC    Subr.   N/A    NetCDF Point output post-processor.
!      GXEXPO    Subr.   N/A    GrADS point output post-processor.
!     ----------------------------------------------------------------
!
!  6. Error messages :
!
!     None.
!
!  7. Remarks :
!
!     If ice concentration is zero, no calculations are made.
!
!  8. Structure :
!
!     See source code.
!
!  9. Switches :
!
!     !/S  Enable subroutine tracing.
!     !/T   Enable general test output.
!           2-D print plot of source term.
!
! 10. Source code :
!
!/ ------------------------------------------------------------------- /
      USE W3ODATMD, ONLY: NDSE
      USE CONSTANTS, ONLY: TPIINV
      USE W3SERVMD, ONLY: EXTCDE
      USE W3GDATMD, ONLY: NK, NTH, NSPEC, SIG, SIG2, DDEN2, IS2PARS
      USE W3GDATMD, ONLY: DTMIN, TH, DTH, ECOS, DTMIN
!/T      USE W3ODATMD, ONLY: NDST
!/S      USE W3SERVMD, ONLY: STRACE
!/T      USE W3ARRYMD, ONLY: PRT2DS
!
      IMPLICIT NONE
!/
!/ ------------------------------------------------------------------- /
!/ Parameter list
      REAL, INTENT(IN)        :: A(NSPEC), CICE, ICEH
      REAL, INTENT(OUT)       :: S(NSPEC), D(NSPEC)
      REAL, INTENT(INOUT)     :: ICEDAVE
!/
!/ ------------------------------------------------------------------- /
!/ Local parameters
!/
!/S      INTEGER, SAVE           :: IENT = 0
      INTEGER                 :: IK, ITH, ITH2, IS, IS2, IND1, IND2, W
      REAL                    :: ALPHA
!/T      REAL                    :: SOUT(NK,NTH)
!/
!/ ------------------------------------------------------------------- /
!/
!/S      CALL STRACE (IENT, 'W3SIS1')
!
! 0.  Initializations ------------------------------------------------ *
!
      S     = 0.
!/T      SOUT  = 0.
!
!
      IND1 = 1+FLOOR((ICEH-THICK1)/DTHICK)
      IND2 = IND1+1
      W = (ICEH-THICK1)/DTHICK - (IND1-1) 
      IF (IND1.LT.1) THEN 
        IND1 = 1
        W = 0
      ELSE IF (IND2.GT.NTHICK) THEN 
        IND2 = NTHICK
        IND1 = NTHICK 
        W = 0 
        END IF

      DO IK = 1,NK
        ALPHA = -1.*(    (SIS2ALPHAS(IND1,IK)*(1-W)+SIS2ALPHAS(IND2,IK)*W)/ICEDAVE &
                    + 2* (SIS2ALPHA2(IND1,IK)*(1-W)+SIS2ALPHA2(IND2,IK)*W) )*CICE
! WRITE(994,*) IK,W,IND1,IND2,SIG(IK)*TPIINV,SIS2ALPHAS(IND1,IK), SIS2ALPHAS(IND2,IK), &
! (SIS2ALPHAS(IND1,IK)*(1-W)+SIS2ALPHAS(IND2,IK)*W), &
! SIS2ALPHA2(IND1,IK)*(1-W)+SIS2ALPHA2(IND2,IK)*W, ALPHA

        DO ITH = 1,NTH
          IS  = ITH+(IK-1)*NTH
          D (IS) = ALPHA
          S(IS)  = ALPHA * A(IS)
          END DO
        END DO
!
!/T         DO IK = 1, NK
!/T           DO ITH = 1, NTH
!/T              IS  = ITH+(IK-1)*NTH
!/T              SOUT(IK,ITH) = S(IS)
!/T           END DO
!/T         END DO
!
!/T            CALL PRT2DS (NDST, NK, NK, NTH, SOUT, SIG(1:NK), '  ', 1.,    &
!/T                               0.0, 0.001, 'Diag Sir1', ' ', 'NONAME')
!
! Formats
      8000 FORMAT (' TEST W3SIS2 : ALPHA :',E10.3)
!
!/
!/ End of W3SIS1 ----------------------------------------------------- /
!/
      END SUBROUTINE W3SIS2
!/
!/ End of module W3SIS1MD -------------------------------------------- /
!/
      END MODULE W3SIS2MD

